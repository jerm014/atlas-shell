#!/usr/bin/env bash
# comment goes here.
#ls /root/checker_functions
#ls /root/checker_functions | tail -c +1001
ls /root/checker_functions | tail -c +2002
ls /root/checker_functions | tail -c +3003
ls /root/checker_functions | tail -c +4004

cat /root/checker_files_repo/README.md
echo "."
find /root -name 104-fibonacci.c
i=1
while [ $i -le 100 ];
do
	if [ $((i%3)) = 0 ] 
	then
		if [ $((i%5)) = 0 ]
		then
	  	   echo "FizzBuzz"
		else
		   echo "Fizz"
		fi
	else
	    if [ $((i%5)) = 0 ]
            then
                echo "Buzz"
	    else
	        echo "$i"
	    fi
	fi
	((i++))
done
